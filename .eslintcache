  <Tab eventKey=\"unanswered\" title=\"Unanswered Questions\">\n                        <ul>\n                            {unansweredQuestions.map(question => (\n                                <li key={question.id}>\n                                    <Question question={question} author={users[question.author]} />\n                                </li>))}\n                        </ul>\n                    </Tab>\n                    <Tab eventKey=\"answered\" title=\"Answered Questions\">\n                        <ul>\n                            {answeredQuestions.map(question => (\n                                <li key={question.id}>\n                                    <Question question={question} author={users[question.author]}/>\n                                </li>))}\n                        </ul>\n                    </Tab>\n                </Tabs>\n            </Fragment>\n        )\n    }\n}\nfunction mapStateToProps({ questions, users, authedUser }) {\n\n    const questionIds =Object.keys(questions)\n                .sort((a, b) => questions[b].timestamp - questions[a].timestamp);\n\n    const unansweredQuestions = questionIds.filter((qid) =>\n        !users[authedUser].answers[qid]).map(qid => questions[qid])\n\n    const answeredQuestions = questionIds.filter((qid) =>\n        users[authedUser].answers[qid]).map(qid => questions[qid])\n\n    return {\n        users,\n        authedUser,\n        unansweredQuestions,\n        answeredQuestions\n    }\n}\nexport default connect(mapStateToProps)(Home);","/media/mohannad/Mohannad's Files/react/react-would-you-rather/src/components/Question.js",[],"/media/mohannad/Mohannad's Files/react/react-would-you-rather/src/utils/helpers.js",[],"/media/mohannad/Mohannad's Files/react/react-would-you-rather/src/components/QuestionPage.js",[],"/media/mohannad/Mohannad's Files/react/react-would-you-rather/src/components/AnswerdQuestion.js",[],"/media/mohannad/Mohannad's Files/react/react-would-you-rather/src/components/UnAnswerdQuestion.js",["114"],"import React, { Component } from 'react'\nimport { connect } from 'react-redux';\nimport { Form, Card, Image, Row, Button } from 'react-bootstrap';\nimport { formatDate } from '../utils/helpers'\nimport {handleAnswerQuestion} from '../actions/questions'\n\nclass UnAnswerdQuestion extends Component{\n    handleSubmit = (e, id)=>{\n        e.preventDefault();\n        const {dispatch} = this.props\n        dispatch(handleAnswerQuestion(id, this.form.answer.value))\n    }\n    render(){\n        const { question, authedUser, author } = this.props\n        return (\n            <Row className='justify-content-center mt-4'>\n                <Card style={{ width: '40rem' }} >\n                    <Card.Header>\n                        <Image\n                            src={author.avatarURL}\n                            roundedCircle\n                            fluid\n                            width=\"40\"\n                            height=\"40\"\n                            className=\"mx-3\"\n                            alt=\"user avatar\"\n                        />\n                        <span>{author.name} asks:</span>\n                    </Card.Header>\n                    <Card.Body className=\"d-flex justify-content-center\">\n                        <Form onSubmit={(e) => this.handleSubmit(e, question.id)}\n                        ref={(form) => (this.form = form)} >\n                            <Form.Check\n                                custom\n                                type=\"radio\"\n                                id=\"optionOne\"\n                                label={question.optionOne.text}\n                                value=\"optionOne\"\n                                name=\"answer\"\n                                className=\"mb-2\"\n                            />\n                            <Form.Check\n                                custom\n                                type=\"radio\"\n                                id=\"optionTwo\"\n                                label={question.optionTwo.text}\n                                value=\"optionTwo\"\n                                name=\"answer\"\n                                className=\"mb-2\"\n                            />\n                            <Button type='submit' variant=\"outline-secondary\">Vote</Button>\n    \n                        </Form>\n                    </Card.Body>\n                    <Card.Footer className=\"text-muted\">{formatDate(question.timestamp)}</Card.Footer>\n                </Card>\n            </Row>\n        )\n    }\n}\n\nfunction mapStateToProps({ authedUser, users, questions }, { id }) {\n    const question = questions[id];\n    const author = users[question.author]\n    return {\n        question,\n        authedUser,\n        author\n    }\n}\n\nexport default connect(mapStateToProps)(UnAnswerdQuestion);","/media/mohannad/Mohannad's Files/react/react-would-you-rather/src/components/NewQuestion.js",["115"],"import { Component } from \"react\";\nimport { Card, Button, Row, Form } from 'react-bootstrap';\nimport { Link, Redirect } from 'react-router-dom';\nimport { connect } from \"react-redux\";\nimport { handleAddQuestion } from '../actions/questions'\n\nclass NewQuestion extends Component {\n\n    state = {\n        optionOne: '',\n        optionTwo: '',\n        toHome: false\n    }\n    handleChange = (e) => {\n        e.preventDefault();\n        const name = e.target.name;\n        const value = e.target.value;\n        this.setState({\n            [name]: value\n        });\n    }\n\n    handleSubmit = (e) => {\n        const { optionOne, optionTwo } = this.state\n        this.props.dispatch(handleAddQuestion({\n            optionOneText: optionOne,\n            optionTwoText: optionTwo\n        }))\n\n        this.setState(() => ({\n            optionOne: '',\n            optionTwo: '',\n            toHome :true\n        }))\n    }\n\n    render() {\n        const { optionOne, optionTwo, toHome } = this.state\n\n\n        if (toHome === true) {\n            return (\n                <Redirect to='/' />\n            )\n        }\n\n        return (\n            <Row className='justify-content-center mt-4'>\n                <Card style={{ width: '40rem' }} >\n                    <Card.Body className=\"text-center\">\n                        <Form onSubmit={this.handleSubmit}>\n                            <Form.Group controlId=\"optionOne\">\n                                <Form.Label>Option One</Form.Label>\n                                <Form.Control\n                                    type=\"text\"\n                                    name=\"optionOne\"\n                                    value={optionOne}\n                                    onChange={this.handleChange}\n                                />\n                            </Form.Group>\n                            <h3>\n                                <small>OR</small>\n                            </h3>\n                            <Form.Group controlId=\"optionTwo\">\n                                <Form.Label>Option Two</Form.Label>\n                                <Form.Control\n                                    type=\"text\"\n                                    name=\"optionTwo\"\n                                    value={optionTwo}\n                                    onChange={this.handleChange}\n                                />\n                            </Form.Group>\n                            <Button\n                                type=\"submit\"\n                                variant=\"outline-secondary\"\n                                disabled={optionOne === '' || optionTwo === ''}>\n                                Submit\n\t\t\t\t\t\t\t</Button>\n                        </Form>\n                    </Card.Body>\n                </Card>\n            </Row>\n        )\n    }\n}\n\n\n\nexport default connect()(NewQuestion);","/media/mohannad/Mohannad's Files/react/react-would-you-rather/src/components/Leaderboard.js",["116"],"/media/mohannad/Mohannad's Files/react/react-would-you-rather/src/components/LeaderboardUser.js",["117","118","119"],{"ruleId":"120","replacedBy":"121"},{"ruleId":"122","replacedBy":"123"},{"ruleId":"124","severity":1,"message":"125","line":4,"column":8,"nodeType":"126","messageId":"127","endLine":4,"endColumn":13},{"ruleId":"124","severity":1,"message":"128","line":16,"column":15,"nodeType":"126","messageId":"127","endLine":16,"endColumn":18},{"ruleId":"124","severity":1,"message":"129","line":14,"column":27,"nodeType":"126","messageId":"127","endLine":14,"endColumn":37},{"ruleId":"124","severity":1,"message":"130","line":3,"column":10,"nodeType":"126","messageId":"127","endLine":3,"endColumn":14},{"ruleId":"124","severity":1,"message":"131","line":4,"column":10,"nodeType":"126","messageId":"127","endLine":4,"endColumn":13},{"ruleId":"124","severity":1,"message":"132","line":1,"column":17,"nodeType":"126","messageId":"127","endLine":1,"endColumn":26},{"ruleId":"124","severity":1,"message":"133","line":2,"column":16,"nodeType":"126","messageId":"127","endLine":2,"endColumn":22},{"ruleId":"124","severity":1,"message":"130","line":3,"column":10,"nodeType":"126","messageId":"127","endLine":3,"endColumn":14},"no-native-reassign",["134"],"no-negated-in-lhs",["135"],"no-unused-vars","'Login' is defined but never used.","Identifier","unusedVar","'arr' is assigned a value but never used.","'authedUser' is assigned a value but never used.","'Link' is defined but never used.","'Row' is defined but never used.","'Component' is defined but never used.","'Button' is defined but never used.","no-global-assign","no-unsafe-negation"]